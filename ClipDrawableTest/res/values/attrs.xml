<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CoverFlow">
        <attr name="imageWidth" format="dimension" />
        <attr name="imageHeight" format="dimension" />
        <attr name="withReflection" format="boolean" />
        <attr name="reflectionGap" format="dimension" />
        <attr name="imageReflectionRatio" format="float" />
    </declare-styleable>
    <declare-styleable name="FlipImageView">
        <attr name="isAnimated" format="boolean" />
        <attr name="isFlipped" format="boolean" />
        <attr name="flipDrawable" format="reference" />
        <attr name="flipDuration" format="integer" />
        <attr name="flipInterpolator" format="reference" />
        <attr name="flipRotations">
            <flag name="none" value="0" />
            <flag name="x" value="1" />
            <flag name="y" value="2" />
            <flag name="z" value="4" />
        </attr>
        <attr name="reverseRotation" format="boolean"></attr>
    </declare-styleable>
    <declare-styleable name="RoundProgress">
        <attr name="roundColor" format="color" />
        <attr name="roundProgressColor" format="color" />
        <attr name="roundProgressTextColor" format="color" />
        <attr name="roundWidth" format="dimension" />
        <attr name="roundRadius" format="dimension" />
        <attr name="roundProgressTextSize" format="dimension"></attr>
        <attr name="displayPercentage" format="boolean" />
        <attr name="roundMax" format="integer" />
        <attr name="roundProgress" format="integer" />
    </declare-styleable>
    
    
     <declare-styleable name="ProgressWheel">
        <attr name="text" format="string" />
        <attr name="textColor" format="color" />
        <attr name="textSize" format="dimension" />
        <attr name="barColor" format="color" />
        <attr name="rimColor" format="color" />
        <attr name="rimWidth" format="dimension" />
        <attr name="spinSpeed" format="dimension" />
        <attr name="delayMillis" format="integer" />
        <attr name="circleColor" format="color" />
        <attr name="pradius" format="dimension" />
        <attr name="barWidth" format="dimension" />
        <attr name="barLength" format="dimension" />
        <attr name="contourColor" format="color" />
        <attr name="contourSize" format="dimension" />
    </declare-styleable>

     <declare-styleable name="IrregularPic">
         <attr name="src" format="reference|color"></attr>
         <attr name="clip_height" format="dimension"></attr>
     </declare-styleable>
</resources>